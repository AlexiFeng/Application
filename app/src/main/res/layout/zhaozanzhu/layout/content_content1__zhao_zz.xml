<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.alexi.demo0851.zhaozanzhu.Content1_ZhaoZZ"
    tools:showIn="@layout/activity_content1__zhao_zz">

    <TextView
        android:id="@+id/name_zz"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="标题"
        android:textAppearance="@style/TextAppearance.AppCompat.Title"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/club_Name" />

    <TextView
        android:id="@+id/time_zz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="时间"
        android:textAppearance="@style/TextAppearance.AppCompat.Notification"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="23dp"
        tools:layout_conversion_absoluteX="193dp"
        tools:layout_conversion_absoluteY="24dp"
        android:layout_marginTop="53dp"
        app:layout_constraintLeft_toRightOf="@+id/linearLayout3"
        android:layout_marginLeft="9dp"
        app:layout_constraintTop_toBottomOf="@+id/name_zz"
        android:layout_marginStart="9dp" />

    <TextView
        android:id="@+id/place_zz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="地点"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/time_zz"
        app:layout_constraintLeft_toRightOf="@+id/linearLayout3"
        android:layout_marginLeft="9dp"
        android:layout_marginStart="9dp" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        style="@style/common_vertical_division_line_style"
        android:layout_height="60dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:background="#FFA500"
        app:layout_constraintBottom_toBottomOf="@+id/place_zz"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/time_zz"
        app:layout_constraintVertical_bias="0.325"
        android:layout_marginBottom="8dp" />


    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/place_zz"
        tools:layout_editor_absoluteX="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-103dp"
            android:orientation="vertical">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="38dp"
                app:layout_constraintTop_toBottomOf="@+id/place_zz"
                tools:layout_editor_absoluteX="16dp">

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="411dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="79dp"
                    android:text="活动需求:"
                    android:textAppearance="@style/TextAppearance.AppCompat.Notification"
                    android:textColor="@color/colorAccent"
                    app:layout_constraintTop_toBottomOf="@+id/time_zz"
                    tools:layout_conversion_absoluteHeight="16dp"
                    tools:layout_conversion_absoluteWidth="411dp"
                    tools:layout_conversion_absoluteX="0dp"
                    tools:layout_conversion_absoluteY="55dp"
                    tools:layout_editor_absoluteX="0dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/need_zz"
                    android:layout_width="403dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="TextView"
                    android:textAppearance="@style/TextAppearance.AppCompat.Notification"
                    app:layout_constraintTop_toBottomOf="@+id/textView16"
                    tools:layout_conversion_absoluteHeight="16dp"
                    tools:layout_conversion_absoluteWidth="403dp"
                    tools:layout_conversion_absoluteX="8dp"
                    tools:layout_conversion_absoluteY="92dp"
                    tools:layout_editor_absoluteX="8dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:text="我们能提供:"
                    android:textAppearance="@style/TextAppearance.AppCompat.Notification"
                    android:textColor="@color/colorPrimaryDark" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/provide_zz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TextView" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="visible">

                <TextView
                    android:id="@+id/jianjie_Title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="简介:"
                    android:visibility="gone" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="8dp">

                <TextView
                    android:id="@+id/jianjie_ZZ"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:visibility="gone" />
            </TableRow>

            <TableLayout
                android:id="@+id/tempLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableLayout>

        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="gone"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        tools:layout_conversion_absoluteWidth="0dp"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView13"
        android:textAppearance="@style/TextAppearance.AppCompat.Notification"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="gone"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        tools:layout_conversion_absoluteWidth="0dp"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView14"
        android:textAppearance="@style/TextAppearance.AppCompat.Notification"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="gone"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        tools:layout_conversion_absoluteWidth="0dp"
        tools:layout_conversion_absoluteHeight="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/club_Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="海风学社"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/kindDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="文娱 | 2017-10-15"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintLeft_toRightOf="@+id/club_Name"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        style="@style/common_horizontal_division_line_style"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/time_zz"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="@+id/linearLayout3"
        app:layout_constraintRight_toRightOf="parent" />

    <LinearLayout
        style="@style/common_horizontal_division_line_style"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintLeft_toLeftOf="@+id/linearLayout3"
        app:layout_constraintTop_toBottomOf="@+id/place_zz"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="17dp"
        android:layout_height="17dp"
        app:srcCompat="@drawable/club"
        app:layout_constraintRight_toLeftOf="@+id/club_Name"
        android:layout_marginRight="0dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="17dp"
        android:layout_height="17dp"
        app:srcCompat="@drawable/tag"
        app:layout_constraintRight_toLeftOf="@+id/kindDate"
        android:layout_marginRight="0dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

</android.support.constraint.ConstraintLayout>
